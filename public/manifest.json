{
    "name": "Clean Chrome",
    "description": "Choose to filter out certain expletives on your page",
    "version": "1.0",
    "manifest_version": 3,
    "author": "Lawrence Han",

    "action": {
        "default_popup": "./index.html"
    },
    "icons": {
      "16": "./icons/logo16.png",
      "32": "./icons/logo32.png",
      "48": "./icons/logo48.png",
      "128": "./icons/logo128.png"
    },
    "background": {
        "service_worker": "./CleanChrome.js"
    },
    "permissions": [
        "activeTab"
    ]
}